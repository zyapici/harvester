{
  "name": "iprbooks/iprbooks-ebs-sdk",
  "description": "IPR Books SDK",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Andrey Cherkasov",
      "email": "andrey.cherkasov93@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Iprbooks\\Ebs\\Sdk\\": "src/",
      "Iprbooks\\Ebs\\Sdk\\Core\\": "src/core/",
      "Iprbooks\\Ebs\\Sdk\\Models\\": "src/models/",
      "Iprbooks\\Ebs\\Sdk\\Collections\\": "src/collections/",
      "Iprbooks\\Ebs\\Sdk\\Managers\\": "src/managers/"
    }
  },
  "require": {
    "php": "5.3.* || 5.4.* || 5.5.* || 5.6.* || 7.0.* || 7.1.* || 7.2.*",
    "ext-curl": "*",
    "ext-json": "*"
  },
  "require-dev": {
    "firebase/php-jwt": "5.0.0"
  },
  "config": {
    "secure-http": false,
    "sort-packages": true,
    "platform": {
      "php": "7.0"
    }
  }
}